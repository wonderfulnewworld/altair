    <!-- Reserve Skills Section -->
    <section id="altair-reserve-skills" class="outer-container">
        <h2>{{localize "altair.reserveSkills.reserveSkills"}}</h2>
        <table class="reserve-skills-table">
            <!-- Define column widths via colgroup -->
            <colgroup>
                <!-- Make the skillName column occupy about half -->
                <col class="col-skillName" />
                <col class="col-skillEffect" />
                <!-- Smaller columns for the cooldowns -->
                <col class="col-skillCooldown" />
                <col class="col-onCooldown" />
            </colgroup>
            <thead>
                <tr>
                    <th>{{localize "altair.general.skillName"}}</th>
                    <th>{{localize "altair.general.effect"}}</th>
                    <th>{{localize "altair.general.dualCooldown"}}</th>
                    <th>{{localize "altair.general.onCooldown"}}</th>
                </tr>
            </thead>
